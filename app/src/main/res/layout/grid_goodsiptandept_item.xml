<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_margin="0.0dip" >

    <!--整个布局-->
    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:padding="5.0dip"
        android:background="@layout/bg_group"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/tv_tlf06"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_weight="4"
            android:layout_alignParentLeft="true"
            android:maxLines="1"
            android:textColor="@color/commo_text_color"
            android:text="时间"
            android:textSize="15.0sp" />
        <TextView
            android:id="@+id/tv_tlf902"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_weight="4"
            android:layout_alignParentLeft="true"
            android:maxLines="1"
            android:textColor="@color/commo_text_color"
            android:text="仓库"
            android:textSize="15.0sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_tlf907"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:layout_alignParentLeft="true"
                android:maxLines="1"
                android:textColor="@color/commo_text_color"
                android:text="操作类型"
                android:textSize="15.0sp" />

            <TextView
                android:id="@+id/tv_tlf10"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:layout_alignParentLeft="true"
                android:maxLines="1"
                android:textColor="@color/commo_text_color"
                android:text="数量"
                android:textSize="15.0sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_gen02"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:layout_alignParentLeft="true"
                android:maxLines="1"
                android:textColor="@color/commo_text_color"
                android:text="操作人员"
                android:textSize="15.0sp" />

            <TextView
                android:id="@+id/tv_tlf09"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:layout_alignParentLeft="true"
                android:maxLines="1"
                android:textColor="@color/commo_text_color"
                android:text="工号"
                android:textSize="15.0sp" />

        </LinearLayout>

    </LinearLayout>
    <View
        style="@style/line"/>
</FrameLayout>